#include <iostream>
using namespace std;

void mySquare(int aa, int bb, int &result) {
    result = aa * bb;
}

void myVolume(int aaa, int bbb, int hhh, int &answer) {
    int sss;
    mySquare(aaa, bbb, sss);
    answer = sss * hhh;
}

int main()
{
    int x, y, z, k;
    cin >> x >> y >> z;
    myVolume(x, y, z, k);
    cout << "Ans: " << k << "\n";

    return 0;
}


