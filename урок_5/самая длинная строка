#include <string.h>
#include <cstdlib>
#include <iostream>
#include <fstream>
#include <math.h>
using namespace std;


int main() {
    int n;
    cin >> n;
    
    char ans[100];
    strcpy(ans, "\0");
    
    for(int i = 0; i < n; i++) {
		char s[100];
		cin >> s;
		if(strlen(s) > strlen(ans)) {
			strcpy(ans, s);
		}
	}
	
	cout << "Longest: " << ans;

    return 0;
}







